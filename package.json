  "dependencies": {
    "@babel/runtime": "^7.12.1",
    "dayjs": "^1.9.6",
    "dom-to-image": "^2.6.0",
    "esbuild": "^0.11.6",
    "flatpickr": "^4.6.6",
    "gs-clipboard": "^0.0.9",
    "jquery": "^3.6.0",
    "lodash-es": "^4.17.21",
    "numeral": "^2.0.6",
    "pako": "^1.0.11"
  },
  "files": [
    "dist"
  ]
}
